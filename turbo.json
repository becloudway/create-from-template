{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "./tsconfig.json",
    "./.eslintrc.cjs",
    "./.eslintignore",
    "./prettierrc.cjs",
    "./jest.config.js",
    "$CI"
  ],
  "pipeline": {
    "lint" : {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "lint:fix" : {
      "dependsOn": [],
      "outputs": []
    },
    "test" : {
      "dependsOn": [],
      "outputs": []
    },
    "build" : {
      "dependsOn": ["lint", "test"],
      "outputs": ["/dist/**"]
    }
  }
}